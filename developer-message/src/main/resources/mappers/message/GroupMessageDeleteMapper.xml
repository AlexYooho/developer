<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.developer.message.mappers.GroupMessageDeleteMapper">

    <resultMap id="BaseResultMap" type="com.developer.message.pojo.GroupMessageDeletePO"/>

    <sql id="Base_Column_list">
        id, group_id, msg_id, msg_seq, user_id, delete_time, create_time, update_time
    </sql>

    <select id="findMessages" resultMap="BaseResultMap">
        SELECT <include refid="Base_Column_list"/>
            FROM group_message_delete
        WHERE group_id = #{group_id}
        AND user_id = #{user_id};
    </select>

</mapper>